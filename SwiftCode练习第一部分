        var number: Int = 0

        let dic2 = ["one":[1,2,3,4,5],
                    "two":[6,7,8,9,10],
                    "three":[3,6,9,5,7]]
        // å¾ªç¯éå†å­—å…¸ä¸­çš„æ•°ç»„
        for (_, values) in dic2 {// å¦‚æœä¸éœ€è¦ä½¿ç”¨æŸä¸ªå€¼ï¼Œå¯ä»¥ç”¨_å»å¿½ç•¥
            for value in values {
                if value > number {
                    number += 1
                }
            }
        }
        print(number)

        // switchï¼ˆæ”¯æŒä»»ä½•ç±»å‹çš„æ•°æ®å’Œå„ç§çš„æ¯”è¾ƒæ“ä½œï¼Œä¸é™äºæ•´æ•°å’Œç›¸ç­‰çš„æµ‹è¯•ï¼‰
        let fruit = "apple"
        switch fruit {
        case "banana":
            print("This is a banana")
        case "lemon":
            print("This is a lemon")
        case "apple","li":// å¯ä»¥ç”¨é€—å·åˆ†éš”å¤šä¸ªæƒ…å†µï¼Œåªè¦åŒ¹é…å…¶ä¸­ä¸€ä¸ªå°±è¡Œ
            print("This is an apple")
        default:
            print("no fruit")
        }

        // èŒƒå›´åŒ¹é…
        let mCount = 1993
        switch mCount {
        case 0:
            print("mCountåœ¨æ­¤èŒƒå›´å†…--0")
        case 1..<5:
            print("mCountåœ¨æ­¤èŒƒå›´å†…--1..<5")
        case 5..<10:
            print("mCountåœ¨æ­¤èŒƒå›´å†…--5..<10")
        case 1000..<2000:
            print("mCountåœ¨æ­¤èŒƒå›´å†…--1000..<2000")
        default:
            print("mCountéƒ½ä¸åœ¨æ­¤èŒƒå›´å†…")
        }

        // å…ƒç¥–åŒ¹é…-åœ¨switchä¸­æµ‹è¯•å¤šä¸ªå€¼ï¼Œæˆ–è€…ç”¨_æ¥åŒ¹é…ä»»ä½•å¯èƒ½çš„å€¼
        let somePoint = (1, 1)// åæ ‡
        switch somePoint {
        case (0, 0):
            print("\(somePoint) in this origin(0, 0)")
        case (_, 0):
            print("\(somePoint) in this origin(_, 0)")
        case (0, _):
            print("\(somePoint) in this origin(0, _)")
        case (-2...2, -2...2):
            print("\(somePoint) in this origin(-2...2, -2...2)")
        default:
            print("no origin")
        }

        // while...
        var m = 2
        while m < 10 {
            m = m * 2;
        }
        print(m)

        repeat {
            m *= 2
        } while m < 20
        print(m)

        // ..<
        var total = 0
        for i in 0..<4 {// ä»0å¾ªç¯åˆ°3
            total += i
            print(total)
        }

        // é—´éš”ç‰¹å®šçš„æ•°è¿›è¡Œå¾ªç¯ï¼Œä¾‹å¦‚ï¼Œæ¯5ä¸ªæ•°åšä¸€ä¸ªæ ‡è®°
        let minIntervel = 5
        for tickMark in stride(from: 0, to: 20, by: minIntervel) {//è·³è¿‡ä¸éœ€è¦çš„æ ‡è®°
            print("tickMark----\(tickMark)")//0,5,10,15
        }

        for tickMark in stride(from: 0, through: 20, by: minIntervel) {// å°é—­èŒƒå›´
            print("tickMark----\(tickMark)")//0,5,10,15,20
        }




        /*
         //MARK: - 5.æ•°ç»„ã€å­—å…¸å’Œé›†åˆ
         æ•°ç»„æ˜¯æœ‰åºå€¼é›†åˆï¼Œå­—å…¸æ˜¯é”®å€¼å…³è”çš„æ— åºé›†åˆï¼Œsetæ˜¯å”¯ä¸€å€¼çš„æ— åºé›†åˆ
         åˆ›å»ºé›†åˆæ—¶ï¼Œå½“æŠŠå®ƒèµ‹å€¼ç»™ä¸€ä¸ªå¸¸é‡ï¼Œå®ƒå°±æ˜¯ä¸å¯å˜çš„ï¼Œå…¶å¤§å°å’Œå†…å®¹ä¸èƒ½ä¿®æ”¹ï¼Œå½“æŠŠå®ƒèµ‹å€¼ç»™ä¸€ä¸ªå˜é‡ï¼Œåˆ™å®ƒæ˜¯å¯å˜çš„ï¼Œå¯ä»¥ä¿®æ”¹åˆ é™¤
         ---------------------------------------
         */

        //--------------*****æ•°ç»„
        // åˆ›å»ºç©ºæ•°ç»„åˆå§‹åŒ–ç¨‹åº,å½“æŒ‡å®šæ•°ç»„ä¸­çš„ç±»å‹æ—¶ï¼Œåˆ™æ•°ç»„ä¸­åªèƒ½å­˜æ”¾è¯¥ç±»å‹çš„æ•°æ®
        var emptyArray = [String]()// å½“èƒ½æ¨æ–­å‡ºç±»å‹æ—¶ï¼Œä¹Ÿå¯ä»¥å†™ä¸ºemptyArray = []

        // åˆ›å»ºç›¸åŒå…ƒç´ ï¼Œå¹¶æŒ‡å®šæ•°é‡çš„æ•°ç»„
        let array = Array(repeating: "å“ˆå–½", count: 5)
        print(array)

        // ä¸¤ä¸ªæ•°ç»„æƒ³åŠ å¯ä»¥åˆ›å»ºæ–°çš„æ•°ç»„
        emptyArray = Array(repeating: "ä½ å¥½å•Š", count: 2)
        let newArray = emptyArray + array
        print(newArray)

        // åˆ›å»ºæ•°ç»„æˆ–å­—å…¸ï¼Œä½¿ç”¨[]ï¼Œæœ€åä¸€ä¸ªå…ƒç´ å…è®¸ç”¨é€—å·ï¼Œè®¿é—®æ•°ç»„æˆ–å­—å…¸ä¸­çš„å…ƒç´ æ—¶é€šè¿‡ç´¢å¼•æˆ–é”®
        var array1 = ["peace","of","love"]
        print(array1)
        print("æ•°ç»„æ˜¯å¦ä¸ºç©º---\(array1.isEmpty)")// æ£€æŸ¥æ•°ç»„countæ˜¯å¦ä¸º0

        // æ•°ç»„æ·»åŠ å…ƒç´ 
        array1.insert("hahha", at: 0)// åœ¨æ•°ç»„æŸä¸ªä½ç½®æ’å…¥æ•°ç»„
        array1.append("other")// åœ¨æ•°ç»„åè¾¹æ‹¼æ¥æ•°ç»„
        array1 += ["æˆ‘æ˜¯è¢«æ·»åŠ çš„"]// ç”¨+=è¿ç®—ç¬¦åˆå¹¶æ•°ç»„
        print("æ·»åŠ ä¹‹åçš„æ•°ç»„ä¸º---\(array1)")// ["hahha", "peace", "of", "love", "other", "æˆ‘æ˜¯è¢«æ·»åŠ çš„"]

        // ä¿®æ”¹æ•°ç»„å…ƒç´ 
        array1[1] = "and"// ä¿®æ”¹æŸä¸ªç´¢å¼•ä¸‹å…ƒç´ çš„å€¼
        array1[0...2] = ["æˆ‘æ˜¯è¢«æ›¿æ¢çš„ï½"]// æ›¿æ¢0.1.2ç´¢å¼•èŒƒå›´çš„å€¼ï¼Œæ›¿æ¢å†…å®¹é•¿åº¦å¯ä»¥ä¸æŒ‡å®šèŒƒå›´å†…ä¸ç›¸ç­‰
        print("ä¿®æ”¹ä¹‹åçš„æ•°ç»„ä¸º---\(array1)")// ["æˆ‘æ˜¯è¢«æ›¿æ¢çš„ï½", "love", "other", "æˆ‘æ˜¯è¢«æ·»åŠ çš„"]

        // åˆ é™¤æ•°ç»„ä¸­çš„å…ƒç´ 
        //        array1.removeAll()// åˆ é™¤å…¨éƒ¨
        //        array1.remove(at: 0)// åˆ é™¤æŸä¸ªå…ƒç´ ï¼Œè¿”å›è¢«åˆ é™¤çš„å…ƒç´ 
        let item = array1.removeLast()// åˆ é™¤æœ€åä¸€ä¸ªå…ƒç´ ï¼Œè¿”å›è¢«åˆ é™¤çš„å…ƒç´ 
        print(item)
        print("åˆ é™¤ä¹‹åçš„æ•°ç»„ä¸º---\(array1)")

        // æ£€æŸ¥æ•°ç»„æ˜¯å¦åŒ…å«æŸä¸ªå…ƒç´ 
        print("æ•°ç»„æ˜¯å¦åŒ…å«love---\(array1.contains("love"))")

        // éå†æ•°ç»„
        for str in array1 {// ä¸å¸¦ç´¢å¼•çš„
            print(str)
        }
        for (index,value) in array1.enumerated() {// enumerated()æ–¹æ³•è¿”å›æ•°ç»„çš„ä¸‹æ ‡å’Œæ‰€åœ¨ä¸‹æ ‡çš„å…ƒç´ ç»„æˆçš„å…ƒç¥–
            print("ç¬¬\(index + 1)ä¸ªå…ƒç´ ä¸º\(value)")
        }

        //--------------*****å­—å…¸
        // åˆ›å»ºç©ºå­—å…¸
        var dic1 = [String: String]()//// å½“èƒ½æ¨æ–­å‡ºç±»å‹æ—¶ï¼Œä¹Ÿå¯ä»¥å†™ä¸ºemptyDic = [:]
        dic1["add"] = "æ·»åŠ "
        print("dic1çš„å€¼ä¸º----\(dic1)")

        dic1 = [:]
        print("dic1åˆè¢«é‡æ–°èµ‹å€¼ä¸ºç©ºå­—å…¸----\(dic1)")

        dic1 = ["name":"lhj",
                "age":"25",
                "gender":"å¥³"]
        dic1["age"] = "24"
        print("dic1è¢«é‡æ–°èµ‹å€¼ä¸º----\(dic1)")

        // è®¿é—®å­—å…¸
        print("dic1çš„æ•°é‡ä¸º\(dic1.count)")// å­—å…¸çš„é”®å€¼å¯¹æ•°é‡
        print("dic1æ˜¯å¦ä¸ºç©º----\(dic1.isEmpty)")//å­—å…¸çš„é”®å€¼å¯¹æ•°é‡æ˜¯å¦ä¸º0

        // å­—å…¸æ·»åŠ é”®å€¼å¯¹
        dic1["new"] = "æ–°çš„"

        // å­—å…¸é€šè¿‡kkeyä¿®æ”¹valueçš„å€¼
        dic1["new"] = "ä¿®æ”¹åæ–°çš„"// å½“æ­¤keyå­˜åœ¨çš„æ—¶å€™ï¼Œåˆ™ä¿®æ”¹keyå¯¹åº”çš„valueå€¼ï¼Œå½“keyä¸å­˜åœ¨çš„æ—¶å€™ï¼Œæ–°å¢é”®å€¼å¯¹
        let oldValue:String! = dic1.updateValue("æˆ‘ä¹Ÿæ˜¯æ–°çš„", forKey: "new")// ä¸ä¸Šé¢æ–¹æ³•ç›¸åŒï¼Œä½†ä¸åŒçš„æ˜¯ï¼Œè¯¥æ–¹æ³•è¿”å›è¢«ä¿®æ”¹çš„æ—§å€¼
        print("è¢«ä¿®æ”¹å‰çš„å€¼ä¸º----\(oldValue!)")
        print("dic1çš„å€¼ä¸º----\(dic1)")

        // åˆ©ç”¨å¯é€‰ç»‘å®šæ£€ç´¢å­—å…¸ä¸­çš„å€¼
        if let valueStr = dic1["newKey"] {
            print("newKeyå¯¹åº”çš„å€¼ä¸º--\(valueStr)")
        } else {
            print("newKeyå¯¹åº”çš„å€¼ä¸å­˜åœ¨")
        }

        // åˆ é™¤æŸä¸ªé”®å€¼å¯¹ï¼Œåªéœ€è¦é€šè¿‡keyæŠŠvalueè®¾ç½®ä¸ºnil
        dic1["new"] = nil
        print("dic1åˆ é™¤newé”®å€¼å¯¹ä¹‹åçš„å€¼ä¸º---\(dic1)")

        // æˆ–è€…ç”¨removeValue forKeyæ–¹æ³•åˆ é™¤æŸä¸ªé”®å€¼å¯¹ï¼Œæ­¤æ–¹æ³•ä¼šè¿”å›è¢«åˆ é™¤çš„å€¼ï¼Œå¦‚æœä¸å­˜åœ¨è¯¥å€¼ï¼Œåˆ™è¿”å›nil
        let removeValue =  dic1.removeValue(forKey: "name")
        print("è¢«åˆ é™¤çš„å€¼ä¸º---\(removeValue!)")

        // éå†å­—å…¸
        for (keyStr, valueStr) in dic1 {// é€šè¿‡å­—å…¸ä¸­çš„æ¯ä¸€é¡¹å…ƒç¥–è¿›è¡Œéå†
            print("\(keyStr)---\(valueStr)")
        }

        for keyStr2 in dic1.keys.sorted() {//
            print("keyStr2----\(keyStr2)")
        }

        for valueStr2 in dic1.values {
            print("value----\(valueStr2)")
        }

        //--------------*****é›†åˆ
        // åˆ›å»ºæŸä¸ªç±»å‹çš„ç©ºé›†åˆï¼Œæ— åºä¸”è‡ªåŠ¨å»é‡
        var newSet = Set<Int>()
        newSet = [1,2,3,4,4,5,6]
        newSet.insert(8)
        newSet.remove(1)
//        newSet.removeAll()
        print("newSetçš„countä¸º---\(newSet.count)\t newSet===\(newSet)")

        print("newSetæ˜¯å¦ä¸ºç©º----\(newSet.isEmpty)")
        print("newSetæ˜¯å¦åŒ…å«5---\(newSet.contains(5))")

        // éå†é›†åˆå¹¶æ’åº
        for intvalue in newSet.sorted() {
            print("------\(intvalue)")
        }

        let aSet: Set = [1,3,5,7,9]
        let bSet: Set = [3,9,2,4,6]
        // intersectionæ–¹æ³•åˆ›å»ºåŒ…å«ä¸¤ä¸ªé›†åˆäº¤é›†çš„æ–°é›†åˆ
        print(aSet.intersection(bSet).sorted())// [3, 9]

        //symmetricDifferenceæ–¹æ³•åˆ›å»ºä¸€ä¸ªåŒ…å«ä»»ä¸€é›†åˆä¸­çš„å€¼ï¼Œä½†ä¸èƒ½åŒ…å«å…¶äº¤é›†
        print(aSet.symmetricDifference(bSet).sorted())// [1, 2, 4, 5, 6, 7]

        // unionæ–¹æ³•åˆ›å»ºåŒ…å«ä¸¤ä¸ªé›†åˆå¹¶é›†çš„æ–°é›†åˆ
        print(aSet.union(bSet).sorted())// [1, 2, 3, 4, 5, 6, 7, 9]

        //subtractingæ–¹æ³•åˆ›å»ºä¸€ä¸ªå€¼ä¸åœ¨æŒ‡å®šé›†åˆçš„æ–°é›†
        print(aSet.subtracting(bSet).sorted())//[1, 5, 7]

        // isSubset ofæ–¹æ³•åˆ¤æ–­é›†åˆçš„æ‰€æœ‰å€¼æ˜¯å¦åŒ…å«åœ¨æŒ‡å®šçš„é›†åˆé‡Œ
        print(aSet.isSubset(of: bSet))

        // isSuperset ofæ–¹æ³•åˆ¤æ–­é›†åˆæ˜¯å¦åŒ…å«æŒ‡å®šé›†åˆä¸­çš„æ‰€æœ‰å€¼
        print(aSet.isSuperset(of: bSet))

        // isStrictSubsetæˆ–isStrictSupersetæ–¹æ³•åˆ¤æ–­é›†åˆæ˜¯å¦æ˜¯æŒ‡å®šé›†åˆçš„å­é›†æˆ–è¶…é›†ï¼Œä½†ä¸ç­‰äº
        print(aSet.isStrictSubset(of: bSet))

        // isDisjoint withæ–¹æ³•åˆ¤æ–­ä¸¤ä¸ªé›†åˆæœ‰æ²¡æœ‰å…±åŒçš„å€¼
        print(aSet.isDisjoint(with: bSet))

        print("\(aSet)----\(bSet)")

    }

}

