        print(mString[mString.index(before: mString.endIndex)])// è®¿é—®æŸä¸ªç´¢å¼•ä¹‹å‰çš„å­—ç¬¦ n
        print(mString[mString.index(after: mString.startIndex)])// è®¿é—®æŸä¸ªç´¢å¼•ä¹‹åçš„å­—ç¬¦ y
        let index = mString.index(mString.startIndex, offsetBy: 3)//ä»æŸä¸ªä¸‹æ ‡å¼€å§‹ï¼Œè·ç¦»ä¸º3çš„ç´¢å¼•
        print("mString----\(mString[index])")// h

